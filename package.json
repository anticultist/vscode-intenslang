{
    "name": "intens-lang",
    "displayName": "INTENS Language",
    "description": "Unofficial syntax definition for the INTENS language",
    "license": "ISC",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/anticultist/vscode-intenslang"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "intens",
                "aliases": [
                    "INTENS",
                    "intens"
                ],
                "extensions": [
                    ".des",
                    ".inc",
                    ".des.in",
                    ".inc.in"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "intens",
                "scopeName": "source.intens",
                "path": "./syntaxes/intens.tmLanguage.json"
            }
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.13",
        "@typescript-eslint/parser": "^5.10.1",
        "copyfiles": "^2.4.1",
        "eslint": "^8.7.0",
        "merge-options": "^3.0.4",
        "mocha": "^9.2.0",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "webpack-cli": "^4.9.2",
        "webpack": "^5.67.0",
        "vscode-tmgrammar-test": "0.0.11"
    }
}
